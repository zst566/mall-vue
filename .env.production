# 生产环境配置
VITE_APP_TITLE=广州商场促销平台
VITE_APP_API_BASE_URL=https://api.example.com/api
VITE_APP_API_TIMEOUT=10000
VITE_APP_ENV=production

# 生产环境开关
VITE_APP_ENABLE_ANALYTICS=true
VITE_APP_ENABLE_LOGGING=true
VITE_APP_ENABLE_DEBUG=false
VITE_APP_ENABLE_MOCK=false

# 微信小程序配置
